{
  "expo": {
    "name": "PlantCare",
    "slug": "plant-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/leaf.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/leaf.png",
      "resizeMode": "contain",
      "backgroundColor": "#fabcff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/leaf.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
